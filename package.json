    "express-rate-limit": "^6.8.0",
    "swagger-ui-express": "^4.7.1",
    "yamljs": "^0.3.0",
    "jsonwebtoken": "^9.0.2",
    "prom-client": "^15.4.0",
    "helmet": "^8.1.0",
    "@opentelemetry/sdk-node": "^0.41.1",
    "@opentelemetry/api": "^1.5.0",
    "@opentelemetry/auto-instrumentations-node": "^0.41.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.41.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
