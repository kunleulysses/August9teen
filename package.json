    "p-limit": "^4.0.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.7",
    "typescript": "^5.3.3",
    "pino": "^8.16.0",
    "@types/express": "^4.17.21",
    "gl-matrix": "^3.4.3",
    "fast-check": "^3.17.0"
