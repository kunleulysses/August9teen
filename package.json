{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:ci": "jest --coverage --runInBand",
    "test:unit": "jest --runInBand",
    "store:migrate": "node scripts/store-migrator.js",
    "start:api": "node server/api/server.js",
    "lint": "eslint .",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "ci": "npm run lint && npm run type-check && npm run test && npm audit --audit-level=moderate",
    "db:push": "prisma db push",
    "postinstall": "prisma db push",
    "dev:redis": "docker run -d -p 6379:6379 redis:7-alpine",
