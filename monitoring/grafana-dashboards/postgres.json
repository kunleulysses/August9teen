{
  "uid": "postgres-overview",
  "title": "Postgres Overview",
  "version": 1,
  "tags": ["postgres", "database"],
  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Postgres Connections",
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "targets": [
        {
          "expr": "pg_connections",
          "legendFormat": "Connections",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Commits",
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 6 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "targets": [
        {
          "expr": "rate(pg_stat_database_xact_commit[5m])",
          "legendFormat": "Commits",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Rollbacks",
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 6 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "targets": [
        {
          "expr": "rate(pg_stat_database_xact_rollback[5m])",
          "legendFormat": "Rollbacks",
          "refId": "A"
        }
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "refresh": 1
      }
    ]
  }
}