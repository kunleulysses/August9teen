{
  "dashboard": {
    "id": null,
    "title": "Self-Coding System Dashboard",
    "tags": ["selfcoding", "consciousness", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Active Generations",
        "type": "stat",
        "targets": [
          {
            "expr": "selfcoding_active_generations",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 2
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Sigil Registry Size",
        "type": "stat",
        "targets": [
          {
            "expr": "sigil_registry_size",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "green",
                  "value": 5
                }
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Code Generation Failures per Minute",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(code_generation_failures_total[1m]) * 60",
            "refId": "A",
            "legendFormat": "Failures/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Sandbox Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(sandbox_timeouts_total[1m]) * 60",
            "refId": "A",
            "legendFormat": "Timeouts/min"
          },
          {
            "expr": "rate(sandbox_memory_limit_exceeded_total[1m]) * 60",
            "refId": "B",
            "legendFormat": "Memory Limits/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Auto-Rollbacks",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(auto_rollbacks_total[1m]) * 60",
            "refId": "A",
            "legendFormat": "Rollbacks/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Quota Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "selfcoding_quota_used",
            "refId": "A",
            "legendFormat": "Used"
          },
          {
            "expr": "selfcoding_quota_limit",
            "refId": "B",
            "legendFormat": "Limit"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "WebSocket Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "consciousness_websocket_connections",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "green",
                  "value": 3
                }
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Generated Modules Total",
        "type": "stat",
        "targets": [
          {
            "expr": "generated_modules_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 100
                }
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 16
        }
      },
      {
        "id": 9,
        "title": "Code Generation Success Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(code_generation_success_total[5m]) / (rate(code_generation_success_total[5m]) + rate(code_generation_failures_total[5m])) * 100",
            "refId": "A",
            "legendFormat": "Success Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 10,
        "title": "System Health Overview",
        "type": "table",
        "targets": [
          {
            "expr": "up{job=~\"consciousness.*\"}",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "color": "red",
                    "text": "Down"
                  },
                  "1": {
                    "color": "green",
                    "text": "Up"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        }
      }
    ],
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(up{job=\"consciousness-system\"}[1m])",
          "iconColor": "blue",
          "titleFormat": "System Restart"
        }
      ]
    }
  }
}
